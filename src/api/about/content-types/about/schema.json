      "required": true
    },
    "size": {
      "type": "string",
      "maxLength": 70
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "red",
        "green",
        "blue ",
        "black",
        "white"
      ],
      "required": true
    },
    "price": {
      "type": "integer",
